<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @member.name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @member.email %>
</p>

<% if session[:role] == "admin" %>
    <%= link_to 'Back', admins_homepage_path %>
<%else%>
    <%= link_to 'Edit', edit_member_path(@member) %>
    <%= link_to 'Back', members_homepage_path%>
<%end%>