<h1>Members with Multiple Reservation permission</h1>

<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @member.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.email %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<% if session[:role] == "admin" %>
    <%= link_to 'Back', admins_homepage_path %>
<%end%>